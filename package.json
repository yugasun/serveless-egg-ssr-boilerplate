{
  "name": "serveless-egg-ssr-boilerplate",
  "version": "4.2.1",
  "description": "基于 easywebpack-react 和 egg-view-react-ssr 插件服务端渲染工程骨架项目",
  "scripts": {
    "clean": "easy clean all",
    "build": "cross-env easy clean && easy build prod",
    "debug": "egg-bin debug",
    "dev": "egg-bin dev",
    "start": "egg-scripts start --daemon --title=egg-react-ssr-boilerplate",
    "stop": "egg-scripts stop --title=egg-react-ssr-boilerplate",
    "restart": "npm run stop && npm run start",
    "backend": "nohup egg-scripts start --port 7001 --workers 4 &",
    "deploy": "npm run build && serverless --debug",
    "lint": "eslint .",
    "fix": "eslint --fix .",
    "ii": "npm install --registry https://registry.npm.taobao.org",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "egg": "^2.1.0",
    "egg-cors": "^2.0.0",
    "egg-logger": "^2.4.1",
    "egg-scripts": "^2.8.1",
    "egg-validate": "^2.0.2",
    "egg-view-react-ssr": "^3.0.0",
    "extend": "~3.0.0",
    "history": "^4.7.2",
    "lodash": "^4.17.4",
    "lodash-id": "^0.14.0",
    "lowdb": "^1.0.0",
    "mockjs": "^1.0.1-beta3",
    "moment": "^2.17.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-loadable": "^5.5.0",
    "react-redux": "^7.1.0",
    "react-router": "^5.0.1",
    "react-router-config": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.1",
    "shortid": "^2.2.8"
  },
  "devDependencies": {
    "@yugasun/easywebpack-cli": "^4.0.0",
    "@yugasun/easywebpack-react": "^4.1.0",
    "conventional-changelog-cli": "^1.3.5",
    "cross-env": "^7.0.0",
    "egg-bin": "^4.5.0",
    "egg-webpack": "^4.4.1",
    "egg-webpack-react": "^3.0.0",
    "eslint-config-egg": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "ip": "^1.1.5",
    "raw-loader": "^4.0.0",
    "service-worker-register": "^1.2.6"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "ci": {
    "version": "8, 10, 12"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yugasun/serveless-egg-ssr-boilerplate.git"
  },
  "author": "hubcarl@126.com",
  "license": "MIT",
  "homepage": "https://github.com/yugasun/serveless-egg-ssr-boilerplate"
}
